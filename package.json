{
    "name": "vscode-tinypg",
    "displayName": "TinyPG",
    "description": "Diagnostic information for projects using TinyPG.",
    "version": "0.0.5",
    "publisher": "joeandaverde",
    "repository": {
        "url": "https://github.com/joeandaverde/vscode-tinypg"
    },
    "bugs": {
        "url": "https://github.com/joeandaverde/vscode-tinypg/issues"
    },
    "engines": {
        "vscode": "^1.5.0"
    },
    "activationEvents": [
        "onLanguage:typescript"
    ],
    "main": "./out/src/extension",
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
        "lodash": "4.17.4",
        "tinypg": "2.0.0-alpha.12",
        "typescript": "^2.0.3"
    },
    "devDependencies": {
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/lodash": "4.14.53",
        "@types/mocha": "^2.2.32"
    }
}